extends layout 

block content 
    h1= title

    p Are you sure you want to delete this instrument record?

    form(method='POST' action='')
        div(class='forms')
            input(type='hidden'
                name='id'
                required='true'
                value=instrument._id)
        button(type='submit' class='options') Yes, delete
        a(href='/catalog' class='options') No
